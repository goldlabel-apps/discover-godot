{
  "name": "discover-godot",
  "version": "1.0.3",
  "description": "Godot is a Free and open source 2D and 3D game engine. React is quite good too",
  "scripts": {
    "start": "$PWD/.scripts/start",
    "lint:node": "cd ./node-app && npx eslint --ext .js,.ts . --fix",
    "dist": "npm run build:react && $PWD/.scripts/dist 1.0.3",
    "build:react": "cd ./react-app && npm run build",
    "deploy": "npm run dist && firebase deploy",
    "test": "cd ./react-app && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/listingslab-software/discover-godot"
  },
  "keywords": [
    "godot",
    "react",
    "casino"
  ],
  "dependencies": {
    "nodemon": "^2.0.15"
  },
  "main": "index.mjs",
  "directories": {
    "doc": "docs"
  }  
}

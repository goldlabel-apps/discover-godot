{
  "name": "discover-godot",
  "version": "1.1.1",
  "description": "Godot Crash Course",
  "scripts": {
    "test": "cd ./apps/react-app && npm run test",
    "start": "$PWD/.scripts/start",
    "dist": "npm run build && $PWD/.scripts/dist 1.1.1",

{
  "name": "discover-godot",
  "version": "1.1.2",
  "description": "Godot Crash Course",
  "scripts": {
    "test": "cd ./apps/react-app && npm run test",
    "start": "$PWD/.scripts/start",
    "dist": "$PWD/.scripts/dist 1.1.2",
    "serve": "serve dist",
    "lint": "cd ./apps/node-app && npx eslint --ext .js,.ts . --fix",
    "build": "cd ./apps/react-app && npm run build",
    "proxy": "cd ./apps/node-app && npm run serve",
    "deploy": "npm run build && npm run dist && firebase deploy --only hosting",
    "deploy:api": "firebase deploy --only functions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/listingslab-software/discover-godot"
  },
  "keywords": [
    "godot",
    "react",
    "pwa",
    "slotmachine",
    "casino"
  ],
  "dependencies": {
    "nodemon": "^2.0.15"
  },
  "main": "index.mjs",
  "directories": {
    "doc": "docs"
  }  
}

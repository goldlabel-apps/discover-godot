  },
  "keywords": [
    "godot",
    "react",
    "pwa",
    "slotmachine",
    "casino"
  ],
  "dependencies": {
    "nodemon": "^2.0.15"
  },
  "main": "index.mjs",
  "directories": {
    "doc": "docs"
  }  
}
